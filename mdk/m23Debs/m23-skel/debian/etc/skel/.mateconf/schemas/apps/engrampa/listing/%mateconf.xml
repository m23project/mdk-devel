<?xml version="1.0"?>
<mateconf>
	<entry name="use_mime_icons" mtime="1367786905" type="schema" stype="bool" owner="engrampa">
		<local_schema locale="C" short_desc="Use mime icons">
			<default type="bool" value="true"/>
			<longdesc>if TRUE will display icons depending on the file type (slower),
	    otherwise will use always the same icon for all files (faster).
	  </longdesc>
		</local_schema>
	</entry>
	<entry name="sort_type" mtime="1367786905" type="schema" stype="string" owner="engrampa">
		<local_schema locale="C" short_desc="Sort type">
			<default type="string">
				<stringvalue>ascending</stringvalue>
			</default>
			<longdesc>Whether to sort in ascending or descending direction. Possible
	    values: ascending, descending.</longdesc>
		</local_schema>
	</entry>
	<entry name="sort_method" mtime="1367786905" type="schema" stype="string" owner="engrampa">
		<local_schema locale="C" short_desc="How to sort files">
			<default type="string">
				<stringvalue>name</stringvalue>
			</default>
			<longdesc>What criteria must be used to arrange files. Possible values:
	    name, size, type, time, path.
	  </longdesc>
		</local_schema>
	</entry>
	<entry name="show_type" mtime="1367786905" type="schema" stype="bool" owner="engrampa">
		<local_schema locale="C" short_desc="Display type">
			<default type="bool" value="true"/>
			<longdesc>Display the Type column in the main window.</longdesc>
		</local_schema>
	</entry>
	<entry name="show_time" mtime="1367786905" type="schema" stype="bool" owner="engrampa">
		<local_schema locale="C" short_desc="Display time">
			<default type="bool" value="true"/>
			<longdesc>Display the Time column in the main window.</longdesc>
		</local_schema>
	</entry>
	<entry name="show_size" mtime="1367786905" type="schema" stype="bool" owner="engrampa">
		<local_schema locale="C" short_desc="Display size">
			<default type="bool" value="true"/>
			<longdesc>Display the Size column in the main window.</longdesc>
		</local_schema>
	</entry>
	<entry name="show_path" mtime="1367786905" type="schema" stype="bool" owner="engrampa">
		<local_schema locale="C" short_desc="Display path">
			<default type="bool" value="true"/>
			<longdesc>Display the Path column in the main window.</longdesc>
		</local_schema>
	</entry>
	<entry name="name_column_width" mtime="1367786905" type="schema" stype="int" owner="engrampa">
		<local_schema locale="C" short_desc="Name column width">
			<default type="int" value="250"/>
			<longdesc>The default width ofthe name column the file list.</longdesc>
		</local_schema>
	</entry>
	<entry name="list_mode" mtime="1367786905" type="schema" stype="string" owner="engrampa">
		<local_schema locale="C" short_desc="List Mode">
			<default type="string">
				<stringvalue>as_folder</stringvalue>
			</default>
			<longdesc>Whether to view all files in the archive (all_files), or
	    view the archive as a folder (as_folder).</longdesc>
		</local_schema>
	</entry>
</mateconf>
