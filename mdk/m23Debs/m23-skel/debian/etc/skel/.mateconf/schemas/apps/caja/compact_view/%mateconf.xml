			<default type="string">
				<stringvalue>standard</stringvalue>
			</default>
			<longdesc>
             Default zoom level used by the compact view.
         </longdesc>
		</local_schema>
	</entry>
	<entry name="all_columns_have_same_width" mtime="1367786905" type="schema" stype="bool" owner="caja">
		<local_schema locale="C" short_desc="All columns have same width">
			<default type="bool" value="true"/>
			<longdesc>
             If this preference is set, all columns in the compact view have the same
	     width. Otherwise, the width of each column is determined seperately.
         </longdesc>
		</local_schema>
	</entry>
