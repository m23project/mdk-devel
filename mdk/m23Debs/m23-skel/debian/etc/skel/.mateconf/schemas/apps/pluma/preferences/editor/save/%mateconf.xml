<?xml version="1.0"?>
<mateconf>
	<entry name="writable_vfs_schemes" mtime="1367786905" type="schema" stype="list" owner="pluma" list_type="string">
		<local_schema locale="C" short_desc="Writable VFS schemes">
			<default type="list" ltype="string">
				<li type="string">
					<stringvalue>dav</stringvalue>
				</li>
				<li type="string">
					<stringvalue>davs</stringvalue>
				</li>
				<li type="string">
					<stringvalue>ftp</stringvalue>
				</li>
				<li type="string">
					<stringvalue>sftp</stringvalue>
				</li>
				<li type="string">
					<stringvalue>smb</stringvalue>
				</li>
				<li type="string">
					<stringvalue>ssh</stringvalue>
				</li>
			</default>
			<longdesc>List of VFS schemes pluma supports in write mode. The &apos;file&apos; scheme
	is writable by default.</longdesc>
		</local_schema>
	</entry>
	<entry name="create_backup_copy" mtime="1367786905" type="schema" stype="bool" owner="pluma">
		<local_schema locale="C" short_desc="Create Backup Copies">
			<default type="bool" value="false"/>
			<longdesc>Whether pluma should create backup copies for the files
	it saves.  You can set the backup file extension with the
	&quot;Backup Copy Extension&quot; option.</longdesc>
		</local_schema>
	</entry>
	<entry name="auto_save_interval" mtime="1367786905" type="schema" stype="int" owner="pluma">
		<local_schema locale="C" short_desc="Autosave Interval">
			<default type="int" value="10"/>
			<longdesc>Number of minutes after which pluma will automatically
	save modified files.  This will only take effect if the &quot;Autosave&quot;
	option is turned on.</longdesc>
		</local_schema>
	</entry>
	<entry name="auto_save" mtime="1367786905" type="schema" stype="bool" owner="pluma">
		<local_schema locale="C" short_desc="Autosave">
			<default type="bool" value="false"/>
			<longdesc>Whether pluma should automatically save modified files
	after a time interval.  You can set the time interval with the
	&quot;Autosave Interval&quot; option.</longdesc>
		</local_schema>
	</entry>
</mateconf>
