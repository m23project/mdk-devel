<?xml version="1.0"?>
<mateconf>
	<entry name="enable" mtime="1367786905" type="schema" stype="bool">
		<local_schema locale="C" short_desc="Whether to thumbnail PCF fonts">
			<default type="bool" value="true"/>
			<longdesc>
          If set to true, then PCF fonts will be thumbnailed.
        </longdesc>
		</local_schema>
	</entry>
	<entry name="command" mtime="1367786905" type="schema" stype="string">
		<local_schema locale="C" short_desc="Thumbnail command for PCF fonts">
			<default type="string">
				<stringvalue>mate-thumbnail-font %u %o</stringvalue>
			</default>
			<longdesc>
          Set this key to the command used to create thumbnails for
          PCF fonts.
        </longdesc>
		</local_schema>
	</entry>
</mateconf>
