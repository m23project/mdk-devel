<?xml version="1.0"?>
<mateconf>
	<entry name="use_custom_radar_url" mtime="1367786905" type="schema" stype="bool" owner="mateweather-applet-2">
		<local_schema locale="C" short_desc="Use custom url for the radar map">
			<default type="bool" value="false"/>
			<longdesc>If true, then retrieve a radar map from a location specified by the &quot;radar&quot; key.</longdesc>
		</local_schema>
	</entry>
	<entry name="temperature_unit" mtime="1367786905" type="schema" stype="string" owner="mateweather-applet-2">
		<local_schema locale="C" short_desc="Temperature unit">
			<default type="string">
				<stringvalue>Default</stringvalue>
			</default>
			<longdesc>The unit to use for temperature.</longdesc>
		</local_schema>
	</entry>
	<entry name="speed_unit" mtime="1367786905" type="schema" stype="string" owner="mateweather-applet-2">
		<local_schema locale="C" short_desc="Speed unit">
			<default type="string">
				<stringvalue>Default</stringvalue>
			</default>
			<longdesc>The unit to use for wind speed.</longdesc>
		</local_schema>
	</entry>
	<entry name="radar" mtime="1367786905" type="schema" stype="string" owner="mateweather-applet-2">
		<local_schema locale="C" short_desc="Url for the radar map">
			<longdesc>The custom url from where to retrieve a radar map.</longdesc>
		</local_schema>
	</entry>
	<entry name="pressure_unit" mtime="1367786905" type="schema" stype="string" owner="mateweather-applet-2">
		<local_schema locale="C" short_desc="Pressure unit">
			<default type="string">
				<stringvalue>Default</stringvalue>
			</default>
			<longdesc>The unit to use for pressure.</longdesc>
		</local_schema>
	</entry>
	<entry name="location4" mtime="1367786905" type="schema" stype="string" owner="mateweather-applet-2">
		<local_schema locale="C" short_desc="Weather for a city">
			<default type="string">
				<stringvalue>DEFAULT_LOCATION</stringvalue>
			</default>
			<longdesc>The city that mateweather displays information for.</longdesc>
		</local_schema>
	</entry>
	<entry name="location3" mtime="1367786905" type="schema" stype="string" owner="mateweather-applet-2">
		<local_schema locale="C" short_desc="Radar location">
			<default type="string">
				<stringvalue>DEFAULT_RADAR</stringvalue>
			</default>
			<longdesc>A three-digit-long code for retrieving radar maps from weather.com, found from http://git.gnome.org/cgit/libmateweather/plain/data/Locations.xml.in</longdesc>
		</local_schema>
	</entry>
	<entry name="location2" mtime="1367786905" type="schema" stype="string" owner="mateweather-applet-2">
		<local_schema locale="C" short_desc="Zone location">
			<default type="string">
				<stringvalue>DEFAULT_ZONE</stringvalue>
			</default>
			<longdesc>A unique zone for the city, as found from http://git.gnome.org/cgit/libmateweather/plain/data/Locations.xml.in</longdesc>
		</local_schema>
	</entry>
	<entry name="location1" mtime="1367786905" type="schema" stype="string" owner="mateweather-applet-2">
		<local_schema locale="C" short_desc="Nearby city">
			<default type="string">
				<stringvalue>DEFAULT_CODE</stringvalue>
			</default>
			<longdesc>Nearby major zone, such as a capital city, as found from http://git.gnome.org/cgit/libmateweather/plain/data/Locations.xml.in</longdesc>
		</local_schema>
	</entry>
	<entry name="location0" mtime="1367786905" type="schema" stype="string" owner="mateweather-applet-2">
		<local_schema locale="C" short_desc="Weather location information">
			<default type="string">
				<stringvalue>DEFAULT_LOCATION</stringvalue>
			</default>
			<longdesc>Weather location information.</longdesc>
		</local_schema>
	</entry>
	<entry name="enable_radar_map" mtime="1367786905" type="schema" stype="bool" owner="mateweather-applet-2">
		<local_schema locale="C" short_desc="Display radar map">
			<default type="bool" value="false"/>
			<longdesc>Fetch a radar map on each update.</longdesc>
		</local_schema>
	</entry>
	<entry name="enable_metric" mtime="1367786905" type="schema" stype="bool" owner="mateweather-applet-2">
		<local_schema locale="C" short_desc="Use metric units">
			<default type="bool" value="false"/>
			<longdesc>Use metric units instead of english units.</longdesc>
		</local_schema>
	</entry>
	<entry name="enable_detailed_forecast" mtime="1367786905" type="schema" stype="bool" owner="mateweather-applet-2">
		<local_schema locale="C" short_desc="Not used anymore">
			<default type="bool" value="false"/>
		</local_schema>
	</entry>
	<entry name="distance_unit" mtime="1367786905" type="schema" stype="string" owner="mateweather-applet-2">
		<local_schema locale="C" short_desc="Distance unit">
			<default type="string">
				<stringvalue>Default</stringvalue>
			</default>
			<longdesc>The unit to use for visibility.</longdesc>
		</local_schema>
	</entry>
	<entry name="coordinates" mtime="1367786905" type="schema" stype="string" owner="mateweather-applet-2">
		<local_schema locale="C" short_desc="Location coordinates">
			<default type="string">
				<stringvalue>DEFAULT_COORDINATES</stringvalue>
			</default>
			<longdesc>Latitude and longitude of your location expressed in DD-MM-SS[NS] DD-MM-SS[EW].</longdesc>
		</local_schema>
	</entry>
	<entry name="auto_update_interval" mtime="1367786905" type="schema" stype="int" owner="mateweather-applet-2">
		<local_schema locale="C" short_desc="Update interval">
			<default type="int" value="1800"/>
			<longdesc>The interval, in seconds, between automatic updates.</longdesc>
		</local_schema>
	</entry>
	<entry name="auto_update" mtime="1367786905" type="schema" stype="bool" owner="mateweather-applet-2">
		<local_schema locale="C" short_desc="Update the data automatically">
			<default type="bool" value="true"/>
			<longdesc>Determines whether the applet automatically updates its weather statistics or not.</longdesc>
		</local_schema>
	</entry>
</mateconf>
