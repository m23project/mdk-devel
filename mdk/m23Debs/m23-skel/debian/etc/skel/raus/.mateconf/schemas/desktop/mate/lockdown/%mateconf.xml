<?xml version="1.0"?>
<mateconf>
	<entry name="disable_user_switching" mtime="1367786905" type="schema" stype="bool" owner="mate">
		<local_schema locale="C" short_desc="Disable user switching">
			<default type="bool" value="false"/>
			<longdesc>
         Prevent the user from switching to another account while his
         session is active.
        </longdesc>
		</local_schema>
	</entry>
	<entry name="disable_save_to_disk" mtime="1367786905" type="schema" stype="bool" owner="mate">
		<local_schema locale="C" short_desc="Disable saving files to disk">
			<default type="bool" value="false"/>
			<longdesc>
          Prevent the user from saving files to disk. For example,
          this would disable access to all applications&apos; &quot;Save as&quot;
          dialogs.
        </longdesc>
		</local_schema>
	</entry>
	<entry name="disable_printing" mtime="1367786905" type="schema" stype="bool" owner="mate">
		<local_schema locale="C" short_desc="Disable printing">
			<default type="bool" value="false"/>
			<longdesc>
          Prevent the user from printing. For example, this would
          disable access to all applications&apos; &quot;Print&quot; dialogs.
        </longdesc>
		</local_schema>
	</entry>
	<entry name="disable_print_setup" mtime="1367786905" type="schema" stype="bool" owner="mate">
		<local_schema locale="C" short_desc="Disable print setup">
			<default type="bool" value="false"/>
			<longdesc>
         Prevent the user from modifying print settings. For example,
         this would disable access to all applications&apos; &quot;Print Setup&quot;
         dialogs.
        </longdesc>
		</local_schema>
	</entry>
	<entry name="disable_lock_screen" mtime="1367786905" type="schema" stype="bool" owner="mate">
		<local_schema locale="C" short_desc="Disable lock screen">
			<default type="bool" value="false"/>
			<longdesc>
         Prevent the user to lock his screen.
        </longdesc>
		</local_schema>
	</entry>
	<entry name="disable_command_line" mtime="1367786905" type="schema" stype="bool" owner="mate">
		<local_schema locale="C" short_desc="Disable command line">
			<default type="bool" value="false"/>
			<longdesc>
          Prevent the user from accessing the terminal or specifying
          a command line to be executed. For example, this would
          disable access to the panel&apos;s &quot;Run Application&quot; dialog.
        </longdesc>
		</local_schema>
	</entry>
	<entry name="disable_application_handlers" mtime="1367786905" type="schema" stype="bool" owner="mate">
		<local_schema locale="C" short_desc="Disable URL and MIME type handlers">
			<default type="bool" value="false"/>
			<longdesc>
         Prevent running any URL or MIME type handler applications.
        </longdesc>
		</local_schema>
	</entry>
</mateconf>
