<?xml version="1.0"?>
<mateconf>
	<entry name="name" mtime="1367786905" type="schema" stype="string" owner="mate-settings-daemon">
		<local_schema locale="C" short_desc="The name of the keyboard shortcut to toggle the on-screen keyboard">
			<default type="string">
				<stringvalue>Toggle on-screen keyboard</stringvalue>
			</default>
			<longdesc>
          This is the name of the keyboard shortcut to toggle the on-screen keyboard. This name will be shown in the keyboard shortcut preferences dialog.
        </longdesc>
		</local_schema>
	</entry>
	<entry name="binding" mtime="1367786905" type="schema" stype="string" owner="mate-settings-daemon">
		<local_schema locale="C" short_desc="Toggle on-screen keyboard">
			<default type="string">
				<stringvalue></stringvalue>
			</default>
			<longdesc>
          Binding to toggle the on-screen keyboard.
        </longdesc>
		</local_schema>
	</entry>
	<entry name="action" mtime="1367786905" type="schema" stype="string" owner="mate-settings-daemon">
		<local_schema locale="C" short_desc="Toggle on-screen keyboard">
			<default type="string">
				<stringvalue>mateconftool-2 --toggle /desktop/mate/applications/at/screen_keyboard_enabled</stringvalue>
			</default>
			<longdesc>
          Command used to turn the on-screen keyboard on or off.
        </longdesc>
		</local_schema>
	</entry>
</mateconf>
