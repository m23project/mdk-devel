<?xml version="1.0"?>
<mateconf>
	<entry name="use_custom_font" mtime="1367786905" type="schema" stype="bool" owner="mate">
		<local_schema locale="C" short_desc="Use Custom Font">
			<default type="bool" value="false"/>
			<longdesc>Whether to use a custom font in gtk+ applications.</longdesc>
		</local_schema>
	</entry>
	<entry name="toolbar_style" mtime="1367786905" type="schema" stype="string" owner="mate">
		<local_schema locale="C" short_desc="Toolbar Style">
			<default type="string">
				<stringvalue>both-horiz</stringvalue>
			</default>
			<longdesc>Toolbar Style. Valid values are &quot;both&quot;, &quot;both-horiz&quot;, &quot;icons&quot;, and
	&quot;text&quot;.</longdesc>
		</local_schema>
	</entry>
	<entry name="toolbar_icons_size" mtime="1367786905" type="schema" stype="string" owner="mate">
		<local_schema locale="C" short_desc="Toolbar Icon Size">
			<default type="string">
				<stringvalue>large-toolbar</stringvalue>
			</default>
			<longdesc>Size of icons in toolbars, either &quot;small-toolbar&quot; or
        &quot;large-toolbar&quot;.</longdesc>
		</local_schema>
	</entry>
	<entry name="toolbar_detachable" mtime="1367786905" type="schema" stype="bool" owner="mate">
		<local_schema locale="C" short_desc="Toolbar Detachable">
			<default type="bool" value="false"/>
			<longdesc>Whether the user can detach toolbars and move them around.</longdesc>
		</local_schema>
	</entry>
	<entry name="status_bar_meter_on_right" mtime="1367786905" type="schema" stype="bool" owner="mate">
		<local_schema locale="C" short_desc="Status Bar on Right">
			<default type="bool" value="false"/>
			<longdesc>Whether to display a status bar meter on the right.</longdesc>
		</local_schema>
	</entry>
	<entry name="show_unicode_menu" mtime="1367786905" type="schema" stype="bool" owner="mate">
		<local_schema locale="C" short_desc="Show the &apos;Unicode Control Character&apos; menu">
			<default type="bool" value="true"/>
			<longdesc>Whether the context menus of entries and text views 
        should offer to insert control characters.</longdesc>
		</local_schema>
	</entry>
	<entry name="show_input_method_menu" mtime="1367786905" type="schema" stype="bool" owner="mate">
		<local_schema locale="C" short_desc="Show the &apos;Input Methods&apos; menu">
			<default type="bool" value="true"/>
			<longdesc>Whether the context menus of entries and text views 
        should offer to change the input method.</longdesc>
		</local_schema>
	</entry>
	<entry name="monospace_font_name" mtime="1367786905" type="schema" stype="string" owner="mate">
		<local_schema locale="C" short_desc="Monospace font">
			<default type="string">
				<stringvalue>Monospace 10</stringvalue>
			</default>
			<longdesc>Name of a monospaced (fixed-width) font for use
              in locations like terminals.</longdesc>
		</local_schema>
	</entry>
	<entry name="menus_have_tearoff" mtime="1367786905" type="schema" stype="bool" owner="mate">
		<local_schema locale="C" short_desc="Menus Have Tearoff">
			<default type="bool" value="false"/>
			<longdesc>Whether menus should have a tearoff.</longdesc>
		</local_schema>
	</entry>
	<entry name="menus_have_icons" mtime="1367786905" type="schema" stype="bool" owner="mate">
		<local_schema locale="C" short_desc="Menus Have Icons">
			<default type="bool" value="true"/>
			<longdesc>Whether menus may display an icon next to a menu entry.</longdesc>
		</local_schema>
	</entry>
	<entry name="menubar_detachable" mtime="1367786905" type="schema" stype="bool" owner="mate">
		<local_schema locale="C" short_desc="Menubar Detachable">
			<default type="bool" value="false"/>
			<longdesc>Whether the user can detach menubars and move them around.</longdesc>
		</local_schema>
	</entry>
	<entry name="menubar_accel" mtime="1367786905" type="schema" stype="string" owner="mate">
		<local_schema locale="C" short_desc="Menubar accelerator">
			<default type="string">
				<stringvalue>F10</stringvalue>
			</default>
			<longdesc>Keyboard shortcut to open the menu bars.</longdesc>
		</local_schema>
	</entry>
	<entry name="icon_theme" mtime="1367786905" type="schema" stype="string" owner="mate">
		<local_schema locale="C" short_desc="Icon Theme">
			<default type="string">
				<stringvalue>mate</stringvalue>
			</default>
			<longdesc>Icon theme to use for the panel, caja etc.</longdesc>
		</local_schema>
	</entry>
	<entry name="gtk_theme" mtime="1367786905" type="schema" stype="string" owner="mate">
		<local_schema locale="C" short_desc="Gtk+ Theme">
			<default type="string">
				<stringvalue>Clearlooks</stringvalue>
			</default>
			<longdesc>Basename of the default theme used by gtk+.</longdesc>
		</local_schema>
	</entry>
	<entry name="gtk_key_theme" mtime="1367786905" type="schema" stype="string" owner="mate">
		<local_schema locale="C" short_desc="Gtk+ Theme">
			<default type="string">
				<stringvalue>Default</stringvalue>
			</default>
			<longdesc>Basename of the default theme used by gtk+.</longdesc>
		</local_schema>
	</entry>
	<entry name="gtk-im-status-style" mtime="1367786905" type="schema" stype="string" owner="mate">
		<local_schema locale="C" short_desc="GTK IM Status Style">
			<default type="string">
				<stringvalue>callback</stringvalue>
			</default>
			<longdesc>Name of the GTK+ input method Status Style used by gtk+.</longdesc>
		</local_schema>
	</entry>
	<entry name="gtk-im-preedit-style" mtime="1367786905" type="schema" stype="string" owner="mate">
		<local_schema locale="C" short_desc="GTK IM Preedit Style">
			<default type="string">
				<stringvalue>callback</stringvalue>
			</default>
			<longdesc>Name of the GTK+ input method Preedit Style used by gtk+.</longdesc>
		</local_schema>
	</entry>
	<entry name="gtk-im-module" mtime="1367786905" type="schema" stype="string" owner="mate">
		<local_schema locale="C" short_desc="GTK IM Module">
			<default type="string">
				<stringvalue></stringvalue>
			</default>
			<longdesc>Name of the input method module used by GTK+.</longdesc>
		</local_schema>
	</entry>
	<entry name="font_name" mtime="1367786905" type="schema" stype="string" owner="mate">
		<local_schema locale="C" short_desc="Default font">
			<default type="string">
				<stringvalue>Sans 10</stringvalue>
			</default>
			<longdesc>Name of the default font used by gtk+.</longdesc>
		</local_schema>
	</entry>
	<entry name="file_chooser_backend" mtime="1367786905" type="schema" stype="string" owner="mate">
		<local_schema locale="C" short_desc="Module for GtkFileChooser">
			<default type="string">
				<stringvalue>gio</stringvalue>
			</default>
			<longdesc>Module to use as the filesystem model for the
	GtkFileChooser widget.  Possible values are &quot;gio&quot;, &quot;mate-vfs&quot; and
	&quot;gtk+&quot;.</longdesc>
		</local_schema>
	</entry>
	<entry name="enable_animations" mtime="1367786905" type="schema" stype="bool" owner="mate">
		<local_schema locale="C" short_desc="Enable Animations">
			<default type="bool" value="true"/>
			<longdesc>Whether animations should be displayed. Note: This is a global
	key, it changes the behaviour of the window manager, the panel etc.
	</longdesc>
		</local_schema>
	</entry>
	<entry name="document_font_name" mtime="1367786905" type="schema" stype="string" owner="mate">
		<local_schema locale="C" short_desc="Document font">
			<default type="string">
				<stringvalue>Sans 10</stringvalue>
			</default>
			<longdesc>Name of the default font used for reading documents.</longdesc>
		</local_schema>
	</entry>
	<entry name="cursor_blink_time" mtime="1367786905" type="schema" stype="int" owner="mate">
		<local_schema locale="C" short_desc="Cursor Blink Time">
			<default type="int" value="1200"/>
			<longdesc>Length of the cursor blink cycle, in milliseconds.</longdesc>
		</local_schema>
	</entry>
	<entry name="cursor_blink" mtime="1367786905" type="schema" stype="bool" owner="mate">
		<local_schema locale="C" short_desc="Cursor Blink">
			<default type="bool" value="true"/>
			<longdesc>Whether the cursor should blink.</longdesc>
		</local_schema>
	</entry>
	<entry name="can_change_accels" mtime="1367786905" type="schema" stype="bool" owner="mate">
		<local_schema locale="C" short_desc="Can Change Accels">
			<default type="bool" value="false"/>
			<longdesc>Whether the user can dynamically type a new accelerator when 
	positioned over an active menuitem.</longdesc>
		</local_schema>
	</entry>
	<entry name="buttons_have_icons" mtime="1367786905" type="schema" stype="bool" owner="mate">
		<local_schema locale="C" short_desc="Buttons Have Icons">
			<default type="bool" value="true"/>
			<longdesc>Whether buttons may display an icon in addition to the button text.</longdesc>
		</local_schema>
	</entry>
	<entry name="accessibility" mtime="1367786905" type="schema" stype="bool" owner="mate">
		<local_schema locale="C" short_desc="Enable Accessibility">
			<default type="bool" value="false"/>
			<longdesc>Whether Applications should have accessibility support.</longdesc>
		</local_schema>
	</entry>
</mateconf>
