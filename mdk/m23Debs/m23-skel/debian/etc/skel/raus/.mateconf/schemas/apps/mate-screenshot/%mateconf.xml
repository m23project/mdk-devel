<?xml version="1.0"?>
<mateconf>
	<entry name="take_window_shot" mtime="1367786905" type="schema" stype="bool" owner="mate-screenshot">
		<local_schema locale="C" short_desc="Window-specific screenshot (deprecated)">
			<default type="bool" value="false"/>
			<longdesc>Grab just the current window, rather than the whole desktop.
        This key has been deprecated and it is no longer in use.</longdesc>
		</local_schema>
	</entry>
	<entry name="last_save_directory" mtime="1367786905" type="schema" stype="string" owner="mate-screenshot">
		<local_schema locale="C" short_desc="Screenshot directory">
			<default type="string">
				<stringvalue></stringvalue>
			</default>
			<longdesc>The directory the last screenshot was saved in.</longdesc>
		</local_schema>
	</entry>
	<entry name="include_pointer" mtime="1367786905" type="schema" stype="bool" owner="mate-screenshot">
		<local_schema locale="C" short_desc="Include Pointer">
			<default type="bool" value="true"/>
			<longdesc>Include the pointer in the screenshot</longdesc>
		</local_schema>
	</entry>
	<entry name="include_border" mtime="1367786905" type="schema" stype="bool" owner="mate-screenshot">
		<local_schema locale="C" short_desc="Include Border">
			<default type="bool" value="true"/>
			<longdesc>Include the window manager border along with the screenshot</longdesc>
		</local_schema>
	</entry>
	<entry name="delay" mtime="1367786905" type="schema" stype="int" owner="mate-screenshot">
		<local_schema locale="C" short_desc="Screenshot delay">
			<default type="int" value="0"/>
			<longdesc>The number of seconds to wait before taking the screenshot.</longdesc>
		</local_schema>
	</entry>
	<entry name="border_effect" mtime="1367786905" type="schema" stype="string" owner="mate-screenshot">
		<local_schema locale="C" short_desc="Border Effect">
			<default type="string">
				<stringvalue>none</stringvalue>
			</default>
			<longdesc>Effect to add to the outside of a border.  Possible values are &quot;shadow&quot;, &quot;none&quot;, and &quot;border&quot;.
      </longdesc>
		</local_schema>
	</entry>
</mateconf>
