<?xml version="1.0"?>
<mateconf>
	<entry name="width" mtime="1367786905" type="schema" stype="int" owner="procman">
		<local_schema locale="C" short_desc="Main Window width">
			<default type="int" value="440"/>
		</local_schema>
	</entry>
	<entry name="view_as" mtime="1367786905" type="schema" stype="int" owner="procman">
		<local_schema locale="C" short_desc="Determines which processes to show by default. 0 is All, 1 is user, and 2 is active">
			<default type="int" value="1"/>
		</local_schema>
	</entry>
	<entry name="update_interval" mtime="1367786905" type="schema" stype="int" owner="procman">
		<local_schema locale="C" short_desc="Time in milliseconds between updates of the process view">
			<default type="int" value="3000"/>
		</local_schema>
	</entry>
	<entry name="swap_color" mtime="1367786905" type="schema" stype="string" owner="procman">
		<local_schema locale="C" short_desc="Default graph swap color">
			<default type="string">
				<stringvalue>#49A835</stringvalue>
			</default>
		</local_schema>
	</entry>
	<entry name="solaris_mode" mtime="1367786905" type="schema" stype="bool" owner="procman">
		<local_schema locale="C" short_desc="Solaris mode for CPU percentage">
			<default type="bool" value="false"/>
			<longdesc>If TRUE, system-monitor operates in &apos;Solaris mode&apos; where a task&apos;s cpu usage is divided by the total number of CPUs. Else it operates in &apos;Irix mode&apos;.</longdesc>
		</local_schema>
	</entry>
	<entry name="smooth_refresh" mtime="1367786905" type="schema" stype="bool" owner="procman">
		<local_schema locale="C" short_desc="Enable/Disable smooth refresh">
			<default type="bool" value="true"/>
		</local_schema>
	</entry>
	<entry name="show_tree" mtime="1367786905" type="schema" stype="bool" owner="procman">
		<local_schema locale="C" short_desc="Show process dependencies in tree form">
			<default type="bool" value="false"/>
		</local_schema>
	</entry>
	<entry name="show_all_fs" mtime="1367786905" type="schema" stype="bool" owner="procman">
		<local_schema locale="C" short_desc="Whether information about all filesystems should be displayed">
			<default type="bool" value="false"/>
			<longdesc>Whether to display information about all filesystems (including types like &apos;autofs&apos; and &apos;procfs&apos;). Useful for getting a list of all currently mounted filesystems.</longdesc>
		</local_schema>
	</entry>
	<entry name="net_out_color" mtime="1367786905" type="schema" stype="string" owner="procman">
		<local_schema locale="C" short_desc="Default graph outgoing network traffic color">
			<default type="string">
				<stringvalue>#844798</stringvalue>
			</default>
		</local_schema>
	</entry>
	<entry name="net_in_color" mtime="1367786905" type="schema" stype="string" owner="procman">
		<local_schema locale="C" short_desc="Default graph incoming network traffic color">
			<default type="string">
				<stringvalue>#2D7DB3</stringvalue>
			</default>
		</local_schema>
	</entry>
	<entry name="mem_color" mtime="1367786905" type="schema" stype="string" owner="procman">
		<local_schema locale="C" short_desc="Default graph mem color">
			<default type="string">
				<stringvalue>#AB1852</stringvalue>
			</default>
		</local_schema>
	</entry>
	<entry name="kill_dialog" mtime="1367786905" type="schema" stype="bool" owner="procman">
		<local_schema locale="C" short_desc="Show warning dialog when killing processes">
			<default type="bool" value="true"/>
		</local_schema>
	</entry>
	<entry name="height" mtime="1367786905" type="schema" stype="int" owner="procman">
		<local_schema locale="C" short_desc="Main Window height">
			<default type="int" value="495"/>
		</local_schema>
	</entry>
	<entry name="graph_update_interval" mtime="1367786905" type="schema" stype="int" owner="procman">
		<local_schema locale="C" short_desc="Time in milliseconds between updates of the graphs">
			<default type="int" value="1000"/>
		</local_schema>
	</entry>
	<entry name="disks_interval" mtime="1367786905" type="schema" stype="int" owner="procman">
		<local_schema locale="C" short_desc="Time in milliseconds between updates of the devices list">
			<default type="int" value="5000"/>
		</local_schema>
	</entry>
	<entry name="current_tab" mtime="1367786905" type="schema" stype="int" owner="procman">
		<local_schema locale="C" short_desc="Saves the currently viewed tab">
			<default type="int" value="2"/>
			<longdesc>0 for the System Info, 1 for the processes list, 2 for the resources and 3 for the disks list</longdesc>
		</local_schema>
	</entry>
	<entry name="cpu_color3" mtime="1367786905" type="schema" stype="string" owner="procman">
		<local_schema locale="C" short_desc="Default graph cpu color">
			<default type="string">
				<stringvalue>#2D7DB3</stringvalue>
			</default>
		</local_schema>
	</entry>
	<entry name="cpu_color2" mtime="1367786905" type="schema" stype="string" owner="procman">
		<local_schema locale="C" short_desc="Default graph cpu color">
			<default type="string">
				<stringvalue>#49A835</stringvalue>
			</default>
		</local_schema>
	</entry>
	<entry name="cpu_color1" mtime="1367786905" type="schema" stype="string" owner="procman">
		<local_schema locale="C" short_desc="Default graph cpu color">
			<default type="string">
				<stringvalue>#CB0C29</stringvalue>
			</default>
		</local_schema>
	</entry>
	<entry name="cpu_color0" mtime="1367786905" type="schema" stype="string" owner="procman">
		<local_schema locale="C" short_desc="Default graph cpu color">
			<default type="string">
				<stringvalue>#FF6E00</stringvalue>
			</default>
		</local_schema>
	</entry>
</mateconf>
