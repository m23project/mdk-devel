<?xml version="1.0"?>
<mateconf>
	<entry name="default_zoom_level" mtime="1367786905" type="schema" stype="string" owner="caja">
		<local_schema locale="C" short_desc="Default list zoom level">
			<default type="string">
				<stringvalue>smaller</stringvalue>
			</default>
			<longdesc>
             Default zoom level used by the list view.
         </longdesc>
		</local_schema>
	</entry>
	<entry name="default_visible_columns" mtime="1367786905" type="schema" stype="list" owner="caja" list_type="string">
		<local_schema locale="C" short_desc="Default list of columns visible in the list view">
			<default type="list" ltype="string">
				<li type="string">
					<stringvalue>name</stringvalue>
				</li>
				<li type="string">
					<stringvalue>size</stringvalue>
				</li>
				<li type="string">
					<stringvalue>type</stringvalue>
				</li>
				<li type="string">
					<stringvalue>date_modified</stringvalue>
				</li>
			</default>
			<longdesc>
             Default list of columns visible in the list view.
         </longdesc>
		</local_schema>
	</entry>
	<entry name="default_sort_order" mtime="1367786905" type="schema" stype="string" owner="caja">
		<local_schema locale="C" short_desc="Default sort order">
			<default type="string">
				<stringvalue>name</stringvalue>
			</default>
			<longdesc>
           The default sort-order for the items in the list view. Possible
	   values are &quot;name&quot;, &quot;size&quot;, &quot;type&quot;, and &quot;modification_date&quot;.
         </longdesc>
		</local_schema>
	</entry>
	<entry name="default_sort_in_reverse_order" mtime="1367786905" type="schema" stype="bool" owner="caja">
		<local_schema locale="C" short_desc="Reverse sort order in new windows">
			<default type="bool" value="false"/>
			<longdesc>
           If true, files in new windows will be sorted in reverse order.
	   ie, if sorted by name, then instead of sorting the files from
	   &quot;a&quot; to &quot;z&quot;, they will be sorted from &quot;z&quot; to &quot;a&quot;.
         </longdesc>
		</local_schema>
	</entry>
	<entry name="default_column_order" mtime="1367786905" type="schema" stype="list" owner="caja" list_type="string">
		<local_schema locale="C" short_desc="Default column order in the list view">
			<default type="list" ltype="string">
				<li type="string">
					<stringvalue>name</stringvalue>
				</li>
				<li type="string">
					<stringvalue>size</stringvalue>
				</li>
				<li type="string">
					<stringvalue>type</stringvalue>
				</li>
				<li type="string">
					<stringvalue>date_modified</stringvalue>
				</li>
			</default>
			<longdesc>
             Default column order in the list view.
         </longdesc>
		</local_schema>
	</entry>
</mateconf>
