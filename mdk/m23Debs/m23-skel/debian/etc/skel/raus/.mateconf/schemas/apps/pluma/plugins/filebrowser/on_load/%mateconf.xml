<?xml version="1.0"?>
<mateconf>
	<entry name="virtual_root" mtime="1367786905" type="schema" stype="string" owner="pluma">
		<local_schema locale="C" short_desc="File Browser Virtual Root Directory">
			<default type="string">
				<stringvalue></stringvalue>
			</default>
			<longdesc>The file browser virtual root directory to use when loading the
	file browser plugin when onload/tree_view is TRUE. The virtual root
	must always be below the actual root.</longdesc>
		</local_schema>
	</entry>
	<entry name="tree_view" mtime="1367786905" type="schema" stype="bool" owner="pluma">
		<local_schema locale="C" short_desc="Open With Tree View">
			<default type="bool" value="true"/>
			<longdesc>Open the tree view when the file browser plugin gets loaded instead of the bookmarks view</longdesc>
		</local_schema>
	</entry>
	<entry name="root" mtime="1367786905" type="schema" stype="string" owner="pluma">
		<local_schema locale="C" short_desc="File Browser Root Directory">
			<default type="string">
				<stringvalue></stringvalue>
			</default>
			<longdesc>The file browser root directory to use when loading the file
	browser plugin and onload/tree_view is TRUE.</longdesc>
		</local_schema>
	</entry>
	<entry name="enable_remote" mtime="1367786905" type="schema" stype="bool" owner="pluma">
		<local_schema locale="C" short_desc="Enable Restore of Remote Locations">
			<default type="bool" value="false"/>
			<longdesc>Sets whether to enable restoring of remote locations.</longdesc>
		</local_schema>
	</entry>
</mateconf>
