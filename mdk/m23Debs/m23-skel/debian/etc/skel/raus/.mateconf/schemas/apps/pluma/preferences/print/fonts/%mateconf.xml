<?xml version="1.0"?>
<mateconf>
	<entry name="print_font_numbers_pango" mtime="1367786905" type="schema" stype="string" owner="pluma">
		<local_schema locale="C" short_desc="Line Number Font for Printing">
			<default type="string">
				<stringvalue>Sans 8</stringvalue>
			</default>
			<longdesc>Specifies the font to use for line numbers when
	printing.  This will only take effect if the &quot;Print Line
	Numbers&quot; option is non-zero.</longdesc>
		</local_schema>
	</entry>
	<entry name="print_font_header_pango" mtime="1367786905" type="schema" stype="string" owner="pluma">
		<local_schema locale="C" short_desc="Header Font for Printing">
			<default type="string">
				<stringvalue>Sans 11</stringvalue>
			</default>
			<longdesc>Specifies the font to use for page headers when printing
	a document. This will only take effect if the &quot;Print Header&quot;
	option is turned on.</longdesc>
		</local_schema>
	</entry>
	<entry name="print_font_body_pango" mtime="1367786905" type="schema" stype="string" owner="pluma">
		<local_schema locale="C" short_desc="Body Font for Printing">
			<default type="string">
				<stringvalue>Monospace 9</stringvalue>
			</default>
			<longdesc>Specifies the font to use for a document&apos;s body when
	printing documents.</longdesc>
		</local_schema>
	</entry>
</mateconf>
