<?xml version="1.0"?>
<mateconf>
	<entry name="touchpad_enabled" mtime="1367786905" type="schema" stype="bool" owner="mate">
		<local_schema locale="C" short_desc="Enable touchpad">
			<default type="bool" value="true"/>
			<longdesc>Set this to TRUE to enable all touchpads.</longdesc>
		</local_schema>
	</entry>
	<entry name="tap_to_click" mtime="1367786905" type="schema" stype="bool" owner="mate">
		<local_schema locale="C" short_desc="Enable mouse clicks with touchpad">
			<default type="bool" value="false"/>
			<longdesc>Set this to TRUE to be able to send mouse clicks by tapping on the touchpad.</longdesc>
		</local_schema>
	</entry>
	<entry name="scroll_method" mtime="1367786905" type="schema" stype="int" owner="mate">
		<local_schema locale="C" short_desc="Select the touchpad scroll method">
			<default type="int" value="1"/>
			<longdesc>Select the touchpad scroll method. Supported values are: 0: disabled, 1: edge scrolling, and 2: two-finger scrolling</longdesc>
		</local_schema>
	</entry>
	<entry name="horiz_scroll_enabled" mtime="1367786905" type="schema" stype="bool" owner="mate">
		<local_schema locale="C" short_desc="Enable horizontal scrolling">
			<default type="bool" value="false"/>
			<longdesc>Set this to TRUE to allow horizontal scrolling by the same method selected with the scroll_method key.</longdesc>
		</local_schema>
	</entry>
	<entry name="disable_while_typing" mtime="1367786905" type="schema" stype="bool" owner="mate">
		<local_schema locale="C" short_desc="Disable touchpad while typing">
			<default type="bool" value="false"/>
			<longdesc>Set this to TRUE if you have problems with accidentally hitting the touchpad while typing.</longdesc>
		</local_schema>
	</entry>
</mateconf>
