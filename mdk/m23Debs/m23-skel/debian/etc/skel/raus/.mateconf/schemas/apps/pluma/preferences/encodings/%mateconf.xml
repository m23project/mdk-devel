<?xml version="1.0"?>
<mateconf>
	<entry name="shown_in_menu" mtime="1367786905" type="schema" stype="list" owner="pluma" list_type="string">
		<local_schema locale="C" short_desc="Encodings shown in menu">
			<default type="list" ltype="string">
				<li type="string">
					<stringvalue>ISO-8859-15</stringvalue>
				</li>
			</default>
			<longdesc>List of encodings shown in the Character Encoding menu in open/save file selector.
	Only recognized encodings are used.</longdesc>
		</local_schema>
	</entry>
	<entry name="auto_detected" mtime="1367786905" type="schema" stype="list" owner="pluma" list_type="string">
		<local_schema locale="C" short_desc="Automatically Detected Encodings">
			<default type="list" ltype="string">
				<li type="string">
					<stringvalue>UTF-8</stringvalue>
				</li>
				<li type="string">
					<stringvalue>CURRENT</stringvalue>
				</li>
				<li type="string">
					<stringvalue>ISO-8859-15</stringvalue>
				</li>
				<li type="string">
					<stringvalue>UTF-16</stringvalue>
				</li>
			</default>
			<longdesc>Sorted list of encodings used by pluma for automatically detecting the encoding of
	a file. &quot;CURRENT&quot; represents the current locale encoding. Only recognized encodings are used.</longdesc>
		</local_schema>
	</entry>
</mateconf>
