<?xml version="1.0"?>
<mateconf>
	<entry name="secondary_color" mtime="1367786905" type="schema" stype="string" owner="mate">
		<local_schema locale="C" short_desc="Secondary Color">
			<default type="string">
				<stringvalue>#4367A5</stringvalue>
			</default>
			<longdesc>Right or Bottom color when drawing gradients, not used for solid color.</longdesc>
		</local_schema>
	</entry>
	<entry name="primary_color" mtime="1367786905" type="schema" stype="string" owner="mate">
		<local_schema locale="C" short_desc="Primary Color">
			<default type="string">
				<stringvalue>#335592</stringvalue>
			</default>
			<longdesc>Left or Top color when drawing gradients, or the solid color.</longdesc>
		</local_schema>
	</entry>
	<entry name="picture_options" mtime="1367786905" type="schema" stype="string" owner="mate">
		<local_schema locale="C" short_desc="Picture Options">
			<default type="string">
				<stringvalue>zoom</stringvalue>
			</default>
			<longdesc>Determines how the image set by wallpaper_filename is
    rendered.  Possible values are &quot;none&quot;, &quot;wallpaper&quot;, &quot;centered&quot;, &quot;scaled&quot;,
    &quot;stretched&quot;, &quot;zoom&quot;, &quot;spanned&quot;.</longdesc>
		</local_schema>
	</entry>
	<entry name="picture_opacity" mtime="1367786905" type="schema" stype="int" owner="mate">
		<local_schema locale="C" short_desc="Picture Opacity">
			<default type="int" value="100"/>
			<longdesc>Opacity with which to draw the background picture.</longdesc>
		</local_schema>
	</entry>
	<entry name="picture_filename" mtime="1367786905" type="schema" stype="string" owner="mate">
		<local_schema locale="C" short_desc="Picture Filename">
			<default type="string">
				<stringvalue>/usr/share/pixmaps/backgrounds/mate/abstract/stripes.jpg</stringvalue>
			</default>
			<longdesc>File to use for the background image.</longdesc>
		</local_schema>
	</entry>
	<entry name="draw_background" mtime="1367786905" type="schema" stype="bool" owner="mate">
		<local_schema locale="C" short_desc="Draw Desktop Background">
			<default type="bool" value="true"/>
			<longdesc>Have MATE draw the desktop background.</longdesc>
		</local_schema>
	</entry>
	<entry name="color_shading_type" mtime="1367786905" type="schema" stype="string" owner="mate">
		<local_schema locale="C" short_desc="Color Shading Type">
			<default type="string">
				<stringvalue>solid</stringvalue>
			</default>
			<longdesc>How to shade the background color. Possible values are &quot;horizontal-gradient&quot;, &quot;vertical-gradient&quot;, and &quot;solid&quot;.</longdesc>
		</local_schema>
	</entry>
</mateconf>
