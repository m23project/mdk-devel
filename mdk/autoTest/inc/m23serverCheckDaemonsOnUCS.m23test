			<bad type="ssh_commandoutput" password="test" cmd="cat /tmp/m.status" description="OpenLDAP ausgefallen">!L0L</bad>
			<bad type="ssh_commandoutput" password="test" cmd="cat /tmp/m.status" description="DHCP-Server ausgefallen">!D0D</bad>
