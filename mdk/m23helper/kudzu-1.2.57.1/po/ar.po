#: ../hwconf.c:126
#, c-format
msgid "ran loadkeys %s"
msgstr "تمّ تشغيل مفاتيح التّحميل %s"

#: ../hwconf.c:533 ../hwconf.c:549 ../hwconf.c:579 ../hwconf.c:611
#, c-format
msgid "aliased %s as %s"
msgstr "سُمّيَ %s بالاسم البديل %s"

#: ../hwconf.c:898
msgid "do only 'safe' probes that won't disturb hardware"
msgstr "قم فقط بالجَسّ الـ'آمن' والذي لا يُزعج العتاد"

#: ../hwconf.c:902
msgid "set timeout in seconds"
msgstr "حدّد وقت الانتهاء بالثّواني"

#: ../hwconf.c:905
msgid "probe only, print information to stdout"
msgstr "جُسّ فقط، واطبع المعلومات إلى stdout"

#: ../hwconf.c:909
msgid "probe only the specified 'bus'"
msgstr "الجَسّ فقط عن 'النّاقل' المُحدّد"

#: ../hwconf.c:913
msgid "probe only for the specified 'class'"
msgstr "الجَسّ فقط عن 'الصّنف' المُحدّد"

#: ../hwconf.c:917
msgid "read probed hardware from a file"
msgstr "اقرأ العتاد المَجسوس من ملفّ"

#: ../hwconf.c:918
msgid "file to read hardware info from"
msgstr "اسم الملفّ لقراءة معلومات العتاد منه"

#: ../hwconf.c:921
msgid "search for modules for a particular kernel version"
msgstr "ابحث عن الوحدات لنُسخة نواة مُعيّنة"

#: ../hwconf.c:922
msgid "kernel version"
msgstr "نسخة النّواة"

#: ../hwconf.c:925
#, fuzzy
msgid "read probed hardware from a socket"
msgstr "اقرأ العتاد المَجسوس من ملفّ"

#: ../hwconf.c:929
msgid "do configuration that doesn't require user input"
msgstr "قم بعمل التّهيئة التي لا تتطلّب مُدخلات من المستخدم"

