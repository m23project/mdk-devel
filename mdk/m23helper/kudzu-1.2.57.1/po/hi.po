#: ../hwconf.c:86
#, c-format
msgid "ran loadkeys %s"
msgstr "%s भारणकुंजियों को चलाया"

#: ../hwconf.c:497 ../hwconf.c:534
#, c-format
msgid "aliased %s as %s"
msgstr "%s को %s के रूप में उपनामित किया गया "

#: ../hwconf.c:737
msgid "do only 'safe' probes that won't disturb hardware"
msgstr "सिर्फ वो 'सुरक्षित' खोज करें जो हार्डवेयर को न छेड़े"

#: ../hwconf.c:741
msgid "set timeout in seconds"
msgstr "समय-समाप्ति को सेकण्ड में जमाएँ"

#: ../hwconf.c:744
msgid "probe only, print information to stdout"
msgstr "सिर्फ खोजें, stdout को जानकारी मुद्रित करें"

#: ../hwconf.c:748
msgid "probe only the specified 'bus'"
msgstr "सिर्फ विशिष्ट 'बस' खोजें"

#: ../hwconf.c:752
msgid "probe only for the specified 'class'"
msgstr "सिर्फ विशिष्ट 'क्लास' के लिए खोजें"

#: ../hwconf.c:756
msgid "read probed hardware from a file"
msgstr "फाइल से खोजी हुई हार्डवेयर जांचें"

#: ../hwconf.c:757
msgid "file to read hardware info from"
msgstr "हार्डवेयर की जानकारी पढने के लिए फाइल"

#: ../hwconf.c:760
msgid "search for modules for a particular kernel version"
msgstr "एक प्रत्येक कर्नल संस्करण के लिए मोड्यूल को जाँचें"

#: ../hwconf.c:761
msgid "kernel version"
msgstr "कर्नल संस्करण"

#: ../hwconf.c:764
msgid "do configuration that doesn't require user input"
msgstr "वैसा विन्यास करें जिसके लिये उपयोक्ता इनपुट की जरूरत नहीं है"

#: ../hwconf.c:786
#, c-format
msgid ""
"\n"
"ERROR - You must be root to run kudzu.\n"
msgstr ""
"\n"
"दोष - कुडज़ू को सिर्फ रूट होकर ही आप चला सकते हैं. \n"

#~ msgid "read probed hardware from a socket"
#~ msgstr "सॉकेट से जांचा गया हार्डवेयर पढ़ा"
