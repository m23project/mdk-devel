#: ../hwconf.c:126
#, c-format
msgid "ran loadkeys %s"
msgstr "изпълнен loadkeys %s"

#: ../hwconf.c:533 ../hwconf.c:549 ../hwconf.c:579 ../hwconf.c:611
#, c-format
msgid "aliased %s as %s"
msgstr "добавен синоним за %s: %s"

#: ../hwconf.c:898
msgid "do only 'safe' probes that won't disturb hardware"
msgstr "извършва само 'безопасни' проби които няма да нарушат работата на хардуера"

#: ../hwconf.c:902
msgid "set timeout in seconds"
msgstr "задава време за изчакване в секунди"

#: ../hwconf.c:905
msgid "probe only, print information to stdout"
msgstr "само проба, извеждане информацията на stdout"

#: ../hwconf.c:909
msgid "probe only the specified 'bus'"
msgstr "проба само на посочен 'bus'"

#: ../hwconf.c:913
msgid "probe only for the specified 'class'"
msgstr "проба само на посочен 'class'"

#: ../hwconf.c:917
msgid "read probed hardware from a file"
msgstr "чете за пробван хардуер от файл"

#: ../hwconf.c:918
msgid "file to read hardware info from"
msgstr "файл от който да се прочете информацията за хардуера"

#: ../hwconf.c:921
msgid "search for modules for a particular kernel version"
msgstr "търсене на модули за определена версия на ядрото"

#: ../hwconf.c:922
msgid "kernel version"
msgstr "версия на ядрото"

#: ../hwconf.c:925
msgid "read probed hardware from a socket"
msgstr "четене за пробван хардуер от сокет"

#: ../hwconf.c:929
msgid "do configuration that doesn't require user input"
msgstr "конфигуриране неизискващо потребителски вход"

#: ../hwconf.c:951
#, c-format
msgid ""
"\n"
"ERROR - You must be root to run kudzu.\n"
msgstr ""
"\n"
"ГРЕШКА - Трябва да сте root за да стартирате kudzu.\n"

