msgid "probe only, print information to stdout"
msgstr "le zaznavanje, izpiši podatke na stdout"

#: ../hwconf.c:909
msgid "probe only the specified 'bus'"
msgstr "zaznavaj le določeno ,vodilo`"

#: ../hwconf.c:913
msgid "probe only for the specified 'class'"
msgstr "zaznavaj le določeni ,razred`"

#: ../hwconf.c:917
msgid "read probed hardware from a file"
msgstr "preberi preskušano opremo iz datoteke"

#: ../hwconf.c:918
msgid "file to read hardware info from"
msgstr "datoteka, s katere naj se preberejo podatki o opremi"

#: ../hwconf.c:921
msgid "search for modules for a particular kernel version"
msgstr "poišči module za določeno različico jedra"

#: ../hwconf.c:922
msgid "kernel version"
msgstr "različica jedra"

#: ../hwconf.c:925
#, fuzzy
msgid "read probed hardware from a socket"
msgstr "preberi preskušano opremo iz datoteke"

#: ../hwconf.c:929
msgid "do configuration that doesn't require user input"
msgstr "izvedi nastavitev, ki ne zahteva uporabniškega vnosa"

