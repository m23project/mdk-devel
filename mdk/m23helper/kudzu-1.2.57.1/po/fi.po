#: ../hwconf.c:126
#, c-format
msgid "ran loadkeys %s"
msgstr "suoritettiin loadkeys %s"

#: ../hwconf.c:533 ../hwconf.c:549 ../hwconf.c:579 ../hwconf.c:611
#, c-format
msgid "aliased %s as %s"
msgstr "annettiin moduulille %s alias %s"

#: ../hwconf.c:898
msgid "do only 'safe' probes that won't disturb hardware"
msgstr "tee vain \"turvallisia\" etsintöjä, jotka eivät häiritse laitteistoa"

#: ../hwconf.c:902
msgid "set timeout in seconds"
msgstr "aseta aikakatkaisu sekunteina"

#: ../hwconf.c:905
msgid "probe only, print information to stdout"
msgstr "tee vain etsintä, tulosta tiedot vakiotulosteeseen"

#: ../hwconf.c:909
msgid "probe only the specified 'bus'"
msgstr "etsi vain annetusta \"väylästä\""

#: ../hwconf.c:913
msgid "probe only for the specified 'class'"
msgstr "etsi vain annetun \"luokan\" laitteita"

#: ../hwconf.c:917
msgid "read probed hardware from a file"
msgstr "lue etsitty laitteisto tiedostosta"

#: ../hwconf.c:918
msgid "file to read hardware info from"
msgstr "tiedosto, josta laitteistotiedot luetaan"

#: ../hwconf.c:921
msgid "search for modules for a particular kernel version"
msgstr "etsi moduuleita tietylle ytimen versiolle"

#: ../hwconf.c:922
msgid "kernel version"
msgstr "ytimen versio"

#: ../hwconf.c:925
msgid "read probed hardware from a socket"
msgstr "lue etsitty laitteisto pistokkeesta"

#: ../hwconf.c:929
msgid "do configuration that doesn't require user input"
msgstr "tee asetukset siten, ettei se vaadi syötettä käyttäjältä"

