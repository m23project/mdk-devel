#: ../hwconf.c:86
#, c-format
msgid "ran loadkeys %s"
msgstr "покренут loadkeys %s"

#: ../hwconf.c:497
#: ../hwconf.c:534
#, c-format
msgid "aliased %s as %s"
msgstr "%s представљен као %s"

#: ../hwconf.c:737
msgid "do only 'safe' probes that won't disturb hardware"
msgstr "извршавај само „сигурне“ тестове који неће узнемиравати хардвер"

#: ../hwconf.c:741
msgid "set timeout in seconds"
msgstr "постави време истека у секундама"

#: ../hwconf.c:744
msgid "probe only, print information to stdout"
msgstr "само тестирај, штампај информације на stdout"

#: ../hwconf.c:748
msgid "probe only the specified 'bus'"
msgstr "тестирај само наведену „магистралу“"

#: ../hwconf.c:752
msgid "probe only for the specified 'class'"
msgstr "тестирај само наведену „класу“"

#: ../hwconf.c:756
msgid "read probed hardware from a file"
msgstr "учитај тестиран хардвер из датотеке"

#: ../hwconf.c:757
msgid "file to read hardware info from"
msgstr "датотека из које се учитавају информације о хардверу"

#: ../hwconf.c:760
msgid "search for modules for a particular kernel version"
msgstr "потражи модуле за одређену верзију језгра"

#: ../hwconf.c:761
msgid "kernel version"
msgstr "верзија језгра"

#: ../hwconf.c:764
msgid "do configuration that doesn't require user input"
msgstr "извршавај подешавања која не захтевају кориснички унос"

#: ../hwconf.c:786
#, c-format
msgid ""
"\n"
"ERROR - You must be root to run kudzu.\n"
msgstr ""
"\n"
"ГРЕШКА - Морате бити root да би покренули kudzu.\n"

