# ../hwconf.c:357
#: ../hwconf.c:86
#, c-format
msgid "ran loadkeys %s"
msgstr "loadkeys %s を実行しました"

# ../hwconf.c:852 ../hwconf.c:897 ../hwconf.c:918 ../hwconf.c:934
# ../hwconf.c:980
#: ../hwconf.c:497 ../hwconf.c:534
#, c-format
msgid "aliased %s as %s"
msgstr "%s の別名として %s を設定しました"

# ../hwconf.c:1403
#: ../hwconf.c:737
msgid "do only 'safe' probes that won't disturb hardware"
msgstr "ハードウェアを干渉しない「安全」検出のみを行う"

# ../hwconf.c:1407
#: ../hwconf.c:741
msgid "set timeout in seconds"
msgstr "待ち時間を秒単位で設定する"

#: ../hwconf.c:744
msgid "probe only, print information to stdout"
msgstr "検出のみ、情報を標準出力に表示する"

#: ../hwconf.c:748
msgid "probe only the specified 'bus'"
msgstr "指定された「バス」のみ検出する"

#: ../hwconf.c:752
msgid "probe only for the specified 'class'"
msgstr "指定された「クラス」のみ検出する"

# ../hwconf.c:1410
#: ../hwconf.c:756
msgid "read probed hardware from a file"
msgstr "検出されたハードウェアをファイルから読み込む"

# ../hwconf.c:1411
#: ../hwconf.c:757
msgid "file to read hardware info from"
msgstr "ハードウェア情報を読み込むファイル"

#: ../hwconf.c:760
msgid "search for modules for a particular kernel version"
msgstr "特定のカーネルバージョン用にモジュールを検索する"

#: ../hwconf.c:761
msgid "kernel version"
msgstr "カーネルバージョン"

# ../hwconf.c:1399
#: ../hwconf.c:764
msgid "do configuration that doesn't require user input"
msgstr "ユーザー入力が不要な設定を行う"

# ../hwconf.c:1422
#: ../hwconf.c:786
#, c-format
msgid ""
"\n"
"ERROR - You must be root to run kudzu.\n"
msgstr ""
"\n"
"エラー - kudzu を実行するには root でなければなりません。\n"

# ../hwconf.c:1410
#~ msgid "read probed hardware from a socket"
#~ msgstr "検出されたハードウェアをソケットから読み込む"
