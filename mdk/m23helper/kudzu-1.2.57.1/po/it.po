#: ../hwconf.c:126
#, c-format
msgid "ran loadkeys %s"
msgstr "eseguito loadkeys %s"

#: ../hwconf.c:532 ../hwconf.c:548 ../hwconf.c:581
#, c-format
msgid "aliased %s as %s"
msgstr "alias di %s come %s"

#: ../hwconf.c:828
msgid "do only 'safe' probes that won't disturb hardware"
msgstr "esegue solo rilevamenti 'sicuri' che non disturbano l'hardware"

#: ../hwconf.c:832
msgid "set timeout in seconds"
msgstr "imposta il timeout in secondi"

#: ../hwconf.c:835
msgid "probe only, print information to stdout"
msgstr "solo rilevamento, stampa le informazioni sullo stdout"

#: ../hwconf.c:839
msgid "probe only the specified 'bus'"
msgstr "rileva solo il 'bus' specificato"

#: ../hwconf.c:843
msgid "probe only for the specified 'class'"
msgstr "rileva solo la 'classe' specificata"

#: ../hwconf.c:847
msgid "read probed hardware from a file"
msgstr "legge le informazioni sull'hardware rilevato da un file"

#: ../hwconf.c:848
msgid "file to read hardware info from"
msgstr "file con le info sull'hardware"

#: ../hwconf.c:851
msgid "search for modules for a particular kernel version"
msgstr "cerca i moduli per una particolare versione del kernel"

#: ../hwconf.c:852
msgid "kernel version"
msgstr "versione del kernel"

#: ../hwconf.c:855
msgid "read probed hardware from a socket"
msgstr "legge l'hardware rilevato da un socket"

#: ../hwconf.c:859
msgid "do configuration that doesn't require user input"
msgstr "fa una configurazione che non richieda input utente"

#: ../hwconf.c:881
#, c-format
msgid ""
"\n"
"ERROR - You must be root to run kudzu.\n"
msgstr ""
"\n"
"ERRORE - Sono necessari i diritti di root per eseguire kudzu.\n"
