#: ../hwconf.c:126
#, c-format
msgid "ran loadkeys %s"
msgstr "laksanakan loadkeys %s"

#: ../hwconf.c:533
#: ../hwconf.c:549
#: ../hwconf.c:579
#: ../hwconf.c:611
#, c-format
msgid "aliased %s as %s"
msgstr "dialiaskan %s sebagai %s"

#: ../hwconf.c:898
msgid "do only 'safe' probes that won't disturb hardware"
msgstr "lakukan hanya pengesanan 'selamat' yang tidak mengusik perkakasan"

#: ../hwconf.c:902
msgid "set timeout in seconds"
msgstr "tetap masatamat dalam saat"

#: ../hwconf.c:905
msgid "probe only, print information to stdout"
msgstr "siasat sahaja, cetak maklumat ke stdout"

#: ../hwconf.c:909
msgid "probe only the specified 'bus'"
msgstr "siasat sahaja 'bas' dinyatakan"

#: ../hwconf.c:913
msgid "probe only for the specified 'class'"
msgstr "kesan hanya untuk 'kelas' dinyatakan"

#: ../hwconf.c:917
msgid "read probed hardware from a file"
msgstr "baca perkakasan dikesan daripada fail"

#: ../hwconf.c:918
msgid "file to read hardware info from"
msgstr "fail untuk maklumat perkakasan dibaca"

#: ../hwconf.c:921
msgid "search for modules for a particular kernel version"
msgstr "cari untuk modul bagi versi kernel tertentu"

#: ../hwconf.c:922
msgid "kernel version"
msgstr "versi kernel"

#: ../hwconf.c:925
msgid "read probed hardware from a socket"
msgstr "baca perkakasan dikesan daripada soket"

#: ../hwconf.c:929
msgid "do configuration that doesn't require user input"
msgstr "lakukan konfigurasi yang tidak memerlukan masukan pengguna"

#: ../hwconf.c:951
