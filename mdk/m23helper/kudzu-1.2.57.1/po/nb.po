#, c-format
msgid "ran loadkeys %s"
msgstr "kjørte loadkeys %s"

#: ../hwconf.c:533 ../hwconf.c:549 ../hwconf.c:579 ../hwconf.c:611
#, c-format
msgid "aliased %s as %s"
msgstr "laget alias %s til %s"

#: ../hwconf.c:898
msgid "do only 'safe' probes that won't disturb hardware"
msgstr "kjør kun 'trygge' søk som ikke forstyrrer maskinvaren"

#: ../hwconf.c:902
msgid "set timeout in seconds"
msgstr "sett tidsavbrudd i sekunder"

#: ../hwconf.c:905
msgid "probe only, print information to stdout"
msgstr "kun søk, skriv ut informasjon til stdout"

#: ../hwconf.c:909
msgid "probe only the specified 'bus'"
msgstr "søk kun på spesifisert 'buss'"

#: ../hwconf.c:913
msgid "probe only for the specified 'class'"
msgstr "søk kun etter spesifisert 'klasse'"

#: ../hwconf.c:917
msgid "read probed hardware from a file"
msgstr "les testet maskinvare fra en fil"

#: ../hwconf.c:918
msgid "file to read hardware info from"
msgstr "fil som maskinvareinformasjon skal leses fra"

#: ../hwconf.c:921
msgid "search for modules for a particular kernel version"
msgstr "søk etter moduler for en bestemt versjon av kjernen"

#: ../hwconf.c:922
msgid "kernel version"
msgstr "kjerneversjoner"

#: ../hwconf.c:925
msgid "read probed hardware from a socket"
msgstr "les gjenkjent maskinvare fra en plugg"

#: ../hwconf.c:929
msgid "do configuration that doesn't require user input"
msgstr "kjør konfigurasjon som ikke behøver input fra bruker"

