#: ../hwconf.c:86
#, c-format
msgid "ran loadkeys %s"
msgstr "%s लोडकिज सुरु केले"

#: ../hwconf.c:497 ../hwconf.c:534
#, c-format
msgid "aliased %s as %s"
msgstr "%s ला %s हे टोपण  ना दिलेले आहेव "

#: ../hwconf.c:737
msgid "do only 'safe' probes that won't disturb hardware"
msgstr "ज्या हार्डवेअरला विस्कळीत करणार नाहीत फक्त अशाच सुरक्षीत तपासण्या करा"

#: ../hwconf.c:741
msgid "set timeout in seconds"
msgstr "सेकंदांमध्ये वेळसमाप्तिची रचना करा"

#: ../hwconf.c:744
msgid "probe only, print information to stdout"
msgstr "फक्त stdout साठी प्रिण्ट करण्याच्या माहितीची तपासनी करा"

#: ../hwconf.c:748
msgid "probe only the specified 'bus'"
msgstr "फक्त नमूद केलेल्या  'bus' ची तपासनी करा"

#: ../hwconf.c:752
msgid "probe only for the specified 'class'"
msgstr "फक्त नमूद केलेल्या  'class' ची तपासनी करा"

#: ../hwconf.c:756
msgid "read probed hardware from a file"
msgstr "तपासनी केलेले हार्डवेअर फाईलमधून वाचा"

#: ../hwconf.c:757
msgid "file to read hardware info from"
msgstr "फाईल जी हार्डवेअरची माहिती वाचण्यासाठी वापरतात"

#: ../hwconf.c:760
msgid "search for modules for a particular kernel version"
msgstr "विशिष्ठ कर्नल आवृत्तीसाठी मॉड्यूलस शोधा"

#: ../hwconf.c:761
msgid "kernel version"
msgstr "कर्नल आवृत्ती"

#: ../hwconf.c:764
msgid "do configuration that doesn't require user input"
msgstr ""

#: ../hwconf.c:786
#, c-format
msgid ""
"\n"
"ERROR - You must be root to run kudzu.\n"
msgstr ""
"\n"
"चूक - kudzu सुरु करण्यासाठी तुम्ही रुटच्या स्वरुपात असणे आवश्यक आहे.\n"

