msgid "set timeout in seconds"
msgstr "усталяваць затрымку ў сэкундах"

#: ../hwconf.c:905
msgid "probe only, print information to stdout"
msgstr "толькі пошук, вывесьці інфармацыю на стандартны вывад"

#: ../hwconf.c:909
msgid "probe only the specified 'bus'"
msgstr "шукаць прыладу толькі на пазначанай шыне ('bus')"

#: ../hwconf.c:913
msgid "probe only for the specified 'class'"
msgstr "шукаць толькі прылады, для якіх пазначаны 'class'"

#: ../hwconf.c:917
msgid "read probed hardware from a file"
msgstr "чытаць інфармацыю аб прыладах з файла"

#: ../hwconf.c:918
msgid "file to read hardware info from"
msgstr "файл для чытаньня інфармацыі аб прыладах"

#: ../hwconf.c:921
msgid "search for modules for a particular kernel version"
msgstr "пошук модуляў для канкрэтнае вэрсіі ядра"

#: ../hwconf.c:922
msgid "kernel version"
msgstr "вэрсія ядра"

#: ../hwconf.c:925
#, fuzzy
msgid "read probed hardware from a socket"
msgstr "чытаць інфармацыю аб прыладах з файла"

#: ../hwconf.c:929
msgid "do configuration that doesn't require user input"
msgstr "наладжваць тое, што не патрабуе ўводу карыстальніка"

