#: ../hwconf.c:126
#, c-format
msgid "ran loadkeys %s"
msgstr "executouse loadkeys %s"

#: ../hwconf.c:533 ../hwconf.c:549 ../hwconf.c:579 ../hwconf.c:611
#, c-format
msgid "aliased %s as %s"
msgstr "fíxose un alias de %s como %s"

#: ../hwconf.c:898
msgid "do only 'safe' probes that won't disturb hardware"
msgstr "facer só deteccións 'seguras' que non interfiran co hardware"

#: ../hwconf.c:902
msgid "set timeout in seconds"
msgstr "establecer tempo de espera en segundos"

#: ../hwconf.c:905
msgid "probe only, print information to stdout"
msgstr "só analisar, escribir a información na saída estándar"

#: ../hwconf.c:909
msgid "probe only the specified 'bus'"
msgstr "analisar únicamente o 'bus' especificado"

#: ../hwconf.c:913
msgid "probe only for the specified 'class'"
msgstr "analisar únicamente a 'clase' especificada"

#: ../hwconf.c:917
msgid "read probed hardware from a file"
msgstr "ler o hardware detectado dende un ficheiro"

#: ../hwconf.c:918
msgid "file to read hardware info from"
msgstr "ficheiro dende o que ler a información do hardware"

#: ../hwconf.c:921
msgid "search for modules for a particular kernel version"
msgstr "buscar módulos dunha versión específica do kernel"

#: ../hwconf.c:922
msgid "kernel version"
msgstr "versión do kernel"

#: ../hwconf.c:925
msgid "read probed hardware from a socket"
msgstr "ler o hardware detectado dende un socket"

#: ../hwconf.c:929
msgid "do configuration that doesn't require user input"
msgstr "configurar sen requirir a intervención do usuario"

