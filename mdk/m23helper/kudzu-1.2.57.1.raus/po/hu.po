#: ../hwconf.c:126
#, c-format
msgid "ran loadkeys %s"
msgstr "loadkeys %s lefuttatva"

#: ../hwconf.c:533 ../hwconf.c:549 ../hwconf.c:579 ../hwconf.c:611
#, c-format
msgid "aliased %s as %s"
msgstr "%s alias neve: %s"

#: ../hwconf.c:898
msgid "do only 'safe' probes that won't disturb hardware"
msgstr ""
"csak biztonságos vizsgálati módok használata (melyek nem állíthatják el a "
"hardvert)"

#: ../hwconf.c:902
msgid "set timeout in seconds"
msgstr "a várakozási idő másodpercben"

#: ../hwconf.c:905
msgid "probe only, print information to stdout"
msgstr "csak vizsgálat; információ kiírása a standard kimenetre (stdout)"

#: ../hwconf.c:909
msgid "probe only the specified 'bus'"
msgstr "csak a megadott busz vizsgálata"

#: ../hwconf.c:913
msgid "probe only for the specified 'class'"
msgstr "csak a megadott 'osztály' vizsgálata"

#: ../hwconf.c:917
msgid "read probed hardware from a file"
msgstr "vizsgált hardvereszközök beolvasása fájlból"

#: ../hwconf.c:918
msgid "file to read hardware info from"
msgstr "hardverinformációt tartalmazó fájl"

#: ../hwconf.c:921
msgid "search for modules for a particular kernel version"
msgstr "modulkeresési útvonal egy adott rednszermag esetén"

#: ../hwconf.c:922
msgid "kernel version"
msgstr "a rendszermag verziószáma"

#: ../hwconf.c:925
msgid "read probed hardware from a socket"
msgstr "vizsgált hardvereszközök beolvasása socketből"

#: ../hwconf.c:929
msgid "do configuration that doesn't require user input"
msgstr "felhasználói beavatkozás nélküli beállítás"

#: ../hwconf.c:951
#, c-format
msgid ""
"\n"
"ERROR - You must be root to run kudzu.\n"
msgstr ""
"\n"
"HIBA - a Kudzu futtatásához rendszergazdai (root) jogosultság szükséges.\n"

