#: ../hwconf.c:898
msgid "do only 'safe' probes that won't disturb hardware"
msgstr "proovi ainult ohutult, mis ei sega riistvara"

#: ../hwconf.c:902
msgid "set timeout in seconds"
msgstr "sea ooteaeg sekundites"

#: ../hwconf.c:905
msgid "probe only, print information to stdout"
msgstr "proovi ainult, väljasta teave standard väljundisse"

#: ../hwconf.c:909
msgid "probe only the specified 'bus'"
msgstr "proovi ainult täpsustatud 'siin'"

#: ../hwconf.c:913
msgid "probe only for the specified 'class'"
msgstr "proovi ainult täpsustatud 'klass'"

#: ../hwconf.c:917
msgid "read probed hardware from a file"
msgstr "loe järele proovitud riistvara failist"

#: ../hwconf.c:918
msgid "file to read hardware info from"
msgstr "riistvara info fail"

#: ../hwconf.c:921
msgid "search for modules for a particular kernel version"
msgstr "otsi mooduleid konkreetse kerneli versiooni jaoks"

#: ../hwconf.c:922
msgid "kernel version"
msgstr "tuuma versioon"

#: ../hwconf.c:925
msgid "read probed hardware from a socket"
msgstr "järele proovitud riistvara loe pesast"

#: ../hwconf.c:929
msgid "do configuration that doesn't require user input"
msgstr "seadista selliselt, et poleks vaja kasutaja määranguid"

