#: ../hwconf.c:126
#, c-format
msgid "ran loadkeys %s"
msgstr "rhedwyd loadkeys %s"

#: ../hwconf.c:533 ../hwconf.c:549 ../hwconf.c:579 ../hwconf.c:611
#, c-format
msgid "aliased %s as %s"
msgstr "ffugenwyd %s yn %s"

#: ../hwconf.c:898
msgid "do only 'safe' probes that won't disturb hardware"
msgstr "gwneud archwiliadau 'diogel' yn unig na fyddant yn tarfu ar galedwedd"

#: ../hwconf.c:902
msgid "set timeout in seconds"
msgstr "gosodwch y goramser mewn eiliadau"

#: ../hwconf.c:905
msgid "probe only, print information to stdout"
msgstr "archwilio'n unig, argraffu gwybodaeth i stdout"

#: ../hwconf.c:909
msgid "probe only the specified 'bus'"
msgstr "archwilio'r 'bws' penodedig yn unig"

#: ../hwconf.c:913
msgid "probe only for the specified 'class'"
msgstr "achwilio ar gyfer y 'dosbarth' penodedig yn unig"

#: ../hwconf.c:917
msgid "read probed hardware from a file"
msgstr "dallen caledwedd archwiliedig o ffeil"

#: ../hwconf.c:918
msgid "file to read hardware info from"
msgstr "ffeil i ddarllen gwybodaeth caledwedd ohoni"

#: ../hwconf.c:921
msgid "search for modules for a particular kernel version"
msgstr "chwilio am fodiwlau ar gyfer fersiwn cnewyllyn penodol"

#: ../hwconf.c:922
msgid "kernel version"
msgstr "fersiwn cnewyllyn"

#: ../hwconf.c:925
msgid "read probed hardware from a socket"
msgstr "dallen caledwedd archwiliedig o soced"

#: ../hwconf.c:929
msgid "do configuration that doesn't require user input"
msgstr "gwneud cyfluniad nad oes angen mewnbwn defnyddiwr"

#: ../hwconf.c:951
#, c-format
msgid ""
"\n"
"ERROR - You must be root to run kudzu.\n"
msgstr ""
"\n"
"GWALL - Rhaid eich bod yn wraidd i redeg kudzu.\n"

