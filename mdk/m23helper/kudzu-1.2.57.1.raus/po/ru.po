#: ../hwconf.c:126
#, c-format
msgid "ran loadkeys %s"
msgstr "выполнено loadkeys %s"

#: ../hwconf.c:533 ../hwconf.c:549 ../hwconf.c:579 ../hwconf.c:611
#, c-format
msgid "aliased %s as %s"
msgstr "%s как псевдоним %s"

#: ../hwconf.c:898
msgid "do only 'safe' probes that won't disturb hardware"
msgstr "использовать только 'безопасные' проверки, не затрагивающие аппаратуру"

#: ../hwconf.c:902
msgid "set timeout in seconds"
msgstr "установить задержку в секундах"

#: ../hwconf.c:905
msgid "probe only, print information to stdout"
msgstr "только поиск, вывести информацию на стандартный вывод"

#: ../hwconf.c:909
msgid "probe only the specified 'bus'"
msgstr "искать устройства только на указанной шине ('bus')"

#: ../hwconf.c:913
msgid "probe only for the specified 'class'"
msgstr "искать только устройства, для которых указан 'class'"

#: ../hwconf.c:917
msgid "read probed hardware from a file"
msgstr "читать информацию о устройствах из файла"

#: ../hwconf.c:918
msgid "file to read hardware info from"
msgstr "файл для чтения информации об устройствах"

#: ../hwconf.c:921
msgid "search for modules for a particular kernel version"
msgstr "поиск модулей для конкретной версии ядра"

#: ../hwconf.c:922
msgid "kernel version"
msgstr "версия ядра"

#: ../hwconf.c:925
msgid "read probed hardware from a socket"
msgstr "читать информацию об устройствах через сокет"

#: ../hwconf.c:929
msgid "do configuration that doesn't require user input"
msgstr "настраивать то, что не требует ввода пользователя"

#: ../hwconf.c:951
#, c-format
msgid ""
"\n"
"ERROR - You must be root to run kudzu.\n"
msgstr ""
"\n"
"ОШИБКА: для выполнения kudzu необходимо иметь права root.\n"

