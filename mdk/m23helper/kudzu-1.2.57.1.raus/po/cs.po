#, c-format
msgid "ran loadkeys %s"
msgstr "spuštěno loadkeys %s"

#: ../hwconf.c:533 ../hwconf.c:549 ../hwconf.c:579 ../hwconf.c:611
#, c-format
msgid "aliased %s as %s"
msgstr "alias %s jako %s"

#: ../hwconf.c:898
msgid "do only 'safe' probes that won't disturb hardware"
msgstr "provést jen 'bezpečné' detekce, které neruší hardware"

#: ../hwconf.c:902
msgid "set timeout in seconds"
msgstr "nastavit časový limit ve vteřinách"

#: ../hwconf.c:905
msgid "probe only, print information to stdout"
msgstr "jen detekovat a vytisknout informace na std. výstup"

#: ../hwconf.c:909
msgid "probe only the specified 'bus'"
msgstr "detekovat jen na specifikované 'sběrnici'"

#: ../hwconf.c:913
msgid "probe only for the specified 'class'"
msgstr "detekovat jen pro specifikovanou 'třídu'"

#: ../hwconf.c:917
msgid "read probed hardware from a file"
msgstr "číst detekovaný hardware ze souboru"

#: ../hwconf.c:918
msgid "file to read hardware info from"
msgstr "soubor s informacemi o hardware"

#: ../hwconf.c:921
msgid "search for modules for a particular kernel version"
msgstr "hledat moduly pro konkrétní verzi jádra"

#: ../hwconf.c:922
msgid "kernel version"
msgstr "verze jádra"

#: ../hwconf.c:925
msgid "read probed hardware from a socket"
msgstr "číst detekovaný hardware ze socketu"

#: ../hwconf.c:929
msgid "do configuration that doesn't require user input"
msgstr "provést nastavení, která nevyžadují vstup od uživatele"

