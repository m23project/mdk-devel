#: ../hwconf.c:86
#, c-format
msgid "ran loadkeys %s"
msgstr "pokreni loadkeys %s"

#: ../hwconf.c:497
#: ../hwconf.c:534
#, c-format
msgid "aliased %s as %s"
msgstr "pseudonim imena %s je %s"

#: ../hwconf.c:737
msgid "do only 'safe' probes that won't disturb hardware"
msgstr "izvedi samo 'sigurna' istraživanja koje neće ometati hardver"

#: ../hwconf.c:741
msgid "set timeout in seconds"
msgstr "zadaj vrijeme isključivanja u sekundama"

#: ../hwconf.c:744
msgid "probe only, print information to stdout"
msgstr "samo istraži, podatke ispiši na stdout"

#: ../hwconf.c:748
msgid "probe only the specified 'bus'"
msgstr "istraži samo određenu sabirnicu"

#: ../hwconf.c:752
msgid "probe only for the specified 'class'"
msgstr "istraži samo za određenu 'klasu'"

#: ../hwconf.c:756
msgid "read probed hardware from a file"
msgstr "čitaj istraženi hardver iz datoteke"

#: ../hwconf.c:757
msgid "file to read hardware info from"
msgstr "datoteka iz koje se čitaju podaci o hardveru"

#: ../hwconf.c:760
msgid "search for modules for a particular kernel version"
msgstr "traži module za određenu verziju kernela"

#: ../hwconf.c:761
msgid "kernel version"
msgstr "verzija kernela"

#: ../hwconf.c:764
msgid "do configuration that doesn't require user input"
msgstr "izradi konfiguraciju koja ne zahtijeva korisnički unos"

#: ../hwconf.c:786
#, c-format
msgid ""
"\n"
"ERROR - You must be root to run kudzu.\n"
msgstr ""
"\n"
"POGREŠKA - Da biste pokrenuli kudzu morate biti root korisnik.\n"

