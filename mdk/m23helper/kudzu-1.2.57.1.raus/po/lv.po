#: ../hwconf.c:126
#, c-format
msgid "ran loadkeys %s"
msgstr "izpildīts loadkeys %s"

#: ../hwconf.c:532 ../hwconf.c:548 ../hwconf.c:581
#, c-format
msgid "aliased %s as %s"
msgstr "%s kā pseidonīms %s"

#: ../hwconf.c:828
msgid "do only 'safe' probes that won't disturb hardware"
msgstr "izmantot tikai 'drošo' pārbaudi', kas neskar aparatūru"

#: ../hwconf.c:832
msgid "set timeout in seconds"
msgstr "uzstādīt aizturi sekundēs"

#: ../hwconf.c:835
msgid "probe only, print information to stdout"
msgstr "tikai pārbaude, informāciju drukāt standarta izvadkanālā"

#: ../hwconf.c:839
msgid "probe only the specified 'bus'"
msgstr "pārbaudīt tikai norādīto slēgumu ('bus')"

#: ../hwconf.c:843
msgid "probe only for the specified 'class'"
msgstr "pārbaudīt tikai norādīto klasi"

#: ../hwconf.c:847
msgid "read probed hardware from a file"
msgstr "informāciju par ierīcēm lasīt no datnes"

#: ../hwconf.c:848
msgid "file to read hardware info from"
msgstr "datne, kas satur informāciju par ierīcēm"

#: ../hwconf.c:851
msgid "search for modules for a particular kernel version"
msgstr "moduļu meklēšana konkrētai kodola versijai"

#: ../hwconf.c:852
msgid "kernel version"
msgstr "kodola versija"

#: ../hwconf.c:855
msgid "read probed hardware from a socket"
msgstr "informāciju par ierīcēm lasīt no ligzdas"

#: ../hwconf.c:859
msgid "do configuration that doesn't require user input"
msgstr "konfigurēt to, kur nav nepieciešama lietotāja iejaukšanās"

#: ../hwconf.c:881
#, c-format
msgid ""
"\n"
"ERROR - You must be root to run kudzu.\n"
msgstr ""
"\n"
"KĻŪDA - Lai lietotu kudzu, Jums jāpieslēdzas kā root.\n"

