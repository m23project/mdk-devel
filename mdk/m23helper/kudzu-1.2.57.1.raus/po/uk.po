#: ../hwconf.c:126
#, c-format
msgid "ran loadkeys %s"
msgstr "виконано loadkeys %s"

#: ../hwconf.c:533 ../hwconf.c:549 ../hwconf.c:579 ../hwconf.c:611
#, c-format
msgid "aliased %s as %s"
msgstr "%s як псевдонім %s"

#: ../hwconf.c:898
msgid "do only 'safe' probes that won't disturb hardware"
msgstr "провадити тільки 'безпечні' проби, які не турбують апаратні засоби "

#: ../hwconf.c:902
msgid "set timeout in seconds"
msgstr "встановити затримку в секундах"

#: ../hwconf.c:905
msgid "probe only, print information to stdout"
msgstr "лише пошук, виводити інформацію у стандартний вивід"

#: ../hwconf.c:909
msgid "probe only the specified 'bus'"
msgstr "шукати пристрої лише на вказаній шині('bus')"

#: ../hwconf.c:913
msgid "probe only for the specified 'class'"
msgstr "шукати лише пристрої, для яких зазначено 'class'"

#: ../hwconf.c:917
msgid "read probed hardware from a file"
msgstr "читати інформацію про пристрої з файлу"

#: ../hwconf.c:918
msgid "file to read hardware info from"
msgstr "файл для читання інформації про пристрої"

#: ../hwconf.c:921
msgid "search for modules for a particular kernel version"
msgstr "пошук модулів для окремої версії ядра"

#: ../hwconf.c:922
msgid "kernel version"
msgstr "версія ядра"

#: ../hwconf.c:925
msgid "read probed hardware from a socket"
msgstr "читати інформацію про пристрої з сокету"

#: ../hwconf.c:929
msgid "do configuration that doesn't require user input"
msgstr "виконувати конфігурацію, яка не вимагає інформації від користувача"

#: ../hwconf.c:951
#, c-format
msgid ""
"\n"
"ERROR - You must be root to run kudzu.\n"
msgstr ""
"\n"
"ПОМИЛКА - для виконання kudzu необхідно мати права root.\n"
