msgid "aliased %s as %s"
msgstr "%s associé à l'alias %s"

#: ../hwconf.c:761
msgid "do only 'safe' probes that won't disturb hardware"
msgstr "procéder uniquement à des détections sans risques pour le matériel"

#: ../hwconf.c:765
msgid "set timeout in seconds"
msgstr "définir un délai d'attente en secondes"

#: ../hwconf.c:768
msgid "probe only, print information to stdout"
msgstr "détecter uniquement, informations d'impression en sortie classique"

#: ../hwconf.c:772
msgid "probe only the specified 'bus'"
msgstr "ne détecter que le 'bus' spécifié"

#: ../hwconf.c:776
msgid "probe only for the specified 'class'"
msgstr "ne détecter que la 'classe' spécifiée"

#: ../hwconf.c:780
msgid "read probed hardware from a file"
msgstr "rechercher le matériel détecté dans un fichier"

#: ../hwconf.c:781
msgid "file to read hardware info from"
msgstr "fichier contenant les informations à lire sur le matériel"

#: ../hwconf.c:784
msgid "search for modules for a particular kernel version"
msgstr "chercher les modules d'une version de noyau particulière"

#: ../hwconf.c:785
msgid "kernel version"
msgstr "version de noyau"

#: ../hwconf.c:788
msgid "read probed hardware from a socket"
msgstr "rechercher le matériel détecté à partir d'un socket"

#: ../hwconf.c:792
msgid "do configuration that doesn't require user input"
msgstr "procéder à une configuration qui ne nécessite aucune saisie manuelle"

