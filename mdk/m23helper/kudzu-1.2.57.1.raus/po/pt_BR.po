#: ../hwconf.c:86
#, c-format
msgid "ran loadkeys %s"
msgstr "executei loadkeys %s"

#: ../hwconf.c:497 ../hwconf.c:534
#, c-format
msgid "aliased %s as %s"
msgstr "apelidei %s de %s"

#: ../hwconf.c:737
msgid "do only 'safe' probes that won't disturb hardware"
msgstr "fazer apenas procuras 'seguras' que não perturbaram o hardware"

#: ../hwconf.c:741
msgid "set timeout in seconds"
msgstr "definir tempo limite em segundos"

#: ../hwconf.c:744
msgid "probe only, print information to stdout"
msgstr "apenas sondar, escrever informações no stdout"

#: ../hwconf.c:748
msgid "probe only the specified 'bus'"
msgstr "sondar apenas o barramento especificado"

#: ../hwconf.c:752
msgid "probe only for the specified 'class'"
msgstr "sondar apenas pela 'classe' especificada"

#: ../hwconf.c:756
msgid "read probed hardware from a file"
msgstr "ler hardware sondado de um arquivo"

#: ../hwconf.c:757
msgid "file to read hardware info from"
msgstr ""
"arquivo a partir do qual as informações sobre o hardware devem ser lidas"

#: ../hwconf.c:760
msgid "search for modules for a particular kernel version"
msgstr "procurar pelos módulos de uma versão específica do kernel"

#: ../hwconf.c:761
msgid "kernel version"
msgstr "versão do kernel"

#: ../hwconf.c:764
msgid "do configuration that doesn't require user input"
msgstr "faça a configuração que não requer uma entrada de usuário"

#: ../hwconf.c:786
#, c-format
msgid ""
"\n"
"ERROR - You must be root to run kudzu.\n"
msgstr ""
"\n"
"ERRO - Você deve ser root para executar o kudzu.\n"

#~ msgid "read probed hardware from a socket"
#~ msgstr "ler hardware sondado a partir de um socket"
